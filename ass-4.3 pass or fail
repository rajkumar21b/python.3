sno=int(input("enter sno:"))
name=str(input("enter student name:"))
group=str(input("enter group:"))
s1=int(input("enter maths marks:"))
s2=int(input("enter c marks:"))
s3=int(input("enter python marks:"))
print("sno:",sno)
print("name:",name)
print("group:",group)
if s1>=35 and s2>=35 and s3>=35 :
    print("pass")
else :
    print("fail")
  
