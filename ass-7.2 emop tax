net=int(input("enter netsalary:"))
if  net>100000 :
    tax=net*10/100
elif net>50000 :
    tax=7
elif net>40000 :
    tax=4
elif net>20000 :
    tax=2
elif net<20000 :
    tax=0
print("tax",tax,"%")

